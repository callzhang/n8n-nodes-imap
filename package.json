{
  "name": "n8n-nodes-imap-enhanced",
  "version": "2.17.7",
  "description": "Enhanced IMAP node with custom labels support, limit parameters, and structured email fields for n8n workflows.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Vasily Maslyukov",
    "email": "auro.coding@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/callzhang/n8n-nodes-imap.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "run-dev": "npm run build && cross-env N8N_LOG_LEVEL=debug n8n start",
    "run-dev-tunnel": "npm run build && cross-env N8N_LOG_LEVEL=debug n8n start --tunnel",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "semantic-release": "semantic-release",
    "test": "cd tests && npm test",
    "test:unit": "npx jest tests/unit",
    "test:integration": "npx jest tests/integration",
    "test:coverage": "npx jest --coverage",
    "test:watch": "npx jest --watch",
    "test:ci": "npx jest --ci --coverage --watchAll=false",
    "test:debug": "npx jest --detectOpenHandles --forceExit",
    "test:run": "node run-tests.js",
    "test:fetchone": "node test-fetchone-uid-parameter.js",
    "test:n8n": "node test-n8n-node-behavior.js",
    "test:regression": "node test-regression-fetchone-uid.js",
    "test:multi": "node test-multi-provider.js",
    "test:jest": "npx jest",
    "test:jest:integration": "npx jest tests/integration",
    "test:jest:unit": "npx jest tests/unit"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ImapCredentials.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Imap/ImapEnhanced.node.js"
    ]
  },
  "devDependencies": {
    "@google/semantic-release-replace-plugin": "^1.2.7",
    "@jest/globals": "^29.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/imapflow": "^1.0.22",
    "@types/jest": "^30.0.0",
    "@types/mailparser": "^3.4.6",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/parser": "~7.18",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.1",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.5",
    "ts-jest": "^29.4.5",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "imap": "^0.8.19",
    "imapflow": "^1.0.188",
    "js-yaml": "^4.1.0",
    "mailparser": "^3.7.3",
    "node-html-markdown": "^1.3.0",
    "nodemailer": "^6.10.1"
  },
  "overrides": {
    "form-data": "^4.0.3"
  }
}
